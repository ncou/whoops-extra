{
    "name": "skm/whoops-extra",
    "description": "Useful handlers (Airbrake, PSR-3, etc) for the popular error handler filp/whoops",
    "license": "MIT",
    "keywords": ["whoops", "airbrake", "psr3"],
    "homepage": "http://github.com/sagikazarmark/whoops-extra",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "filp/whoops": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "airbrake/phpbrake": "^0.2.2",
        "psr/log": "^1.0"
    },
    "suggest": {
        "airbrake/phpbrake": "Allow sending errors to Airbrake",
        "psr/log": "Allow logging errors"
    },
    "autoload": {
        "psr-4": {
            "SKM\\Whoops\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
